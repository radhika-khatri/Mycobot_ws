<launch>
    <node pkg="yolo_object_detection" type="detect_objects.py" name="yolov8_object_detection" output="screen">
        <param name="model_path" value="$(find yolo_object_detection)/models/best.pt" />
    </node>

    <!-- Launch RViz for visualization -->
    <node pkg="rviz" type="rviz" name="rviz" args="-d $(find yolo_object_detection)/rviz/mycobot_object_detection.rviz" />
</launch>
